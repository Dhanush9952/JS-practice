// Assign age
let myAge = 26;
// Assign early year
let earlyYears = 2;
earlyYears *= 10.5;
// later years calculate
laterYears = myAge - 2;
// multiply by 4
laterYears *= 4;
console.log(earlyYears);
console.log(laterYears);
// add both
myAgeInDogYears = earlyYears + laterYears;
// name to lower case
let name1 = "Dhanush";
myName = name1.toLowerCase();
console.log(`My name is ${name1}. I am ${myAge} years old in human years which is ${myAgeInDogYears} years old in dog years.`)
